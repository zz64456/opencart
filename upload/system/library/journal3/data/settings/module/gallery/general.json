    "selector": "div.module-gallery-%s .module-item:hover > a .gallery-image::before",
    "property": "opacity: %s"
  },
  "OverlayDisplay": {
    "type": "Radio",
    "value": "",
    "rules": {
      "hover": {
        "div.module-gallery-%s .module-item > a .gallery-image::before": "visibility: hidden",
        "div.module-gallery-%s .module-item:hover > a .gallery-image::before": "visibility: visible"
