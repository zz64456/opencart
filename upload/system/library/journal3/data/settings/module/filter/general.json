  },
  "itemBorder": {
    "type": "Border",
    "selector": ".module-filter-%s label .filter-item-bg"
  },
  "itemBorderHover": {
    "type": "Color",
    "property": "border-color",
    "selector": ".module-filter-%s label:hover .filter-item-bg"
  },
  "itemBorderActive": {
    "type": "Color",
    "property": "border-color",
    "selector": ".module-filter-%s label input:checked ~ .filter-item-bg"
  },
  "itemShadow": {
    "type": "Shadow",
