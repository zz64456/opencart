{% if (top_messages) %} 
<script>
$(document).ready(function() {
  {% for top_message in top_messages %}
  	ka_extensions.showMessage("{{ top_message['content']|e('js') }} ", "{{ top_message['type'] }}");
  {% endfor %}
}); 	
</script>  
{% endif %}