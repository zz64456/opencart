    "selector": "div.module-gallery-%s .module-item:first-child > a"
  },
  "LastBorderRadius": {
    "type": "BorderRadius",
    "selector": "div.module-gallery-%s .module-item:last-child > a"
  },
  "thumbShadow": {
    "type": "Shadow",
    "selector": "div.module-gallery-%s .module-item > a"
  },
  "thumbShadowHover": {
    "type": "Shadow",
    "selector": "div.module-gallery-%s .module-item > a:hover"
  },
  "thumbPadding": {
    "type": "Padding",
    "selector": "div.module-gallery-%s .module-item > a"
  },
  "thumbBackground": {
    "type": "Background",
    "selector": "div.module-gallery-%s .module-item > a"
  },
  "thumbBackgroundHover": {
    "type": "Background",
    "selector": "div.module-gallery-%s .module-item > a:hover"
  },
  "grayscale": {
    "type": "Radio",
    "value": "",
    "rules": {
      "none": {
        "div.module-gallery-%s .module-item > a img": "filter: grayscale(0)"
      },
      "always": {
        "div.module-gallery-%s .module-item > a img": "filter: grayscale(1)"
