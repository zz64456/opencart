        "div.module-gallery-%s .auto-carousel-controls.auto-carousel-bar": "max-height: none"
      },
      "none": {
        "div.module-gallery-%s .auto-grid": "--scrollbar-top-space: 0px; --scrollbar-bottom-space: 0px",
        "div.module-gallery-%s .auto-carousel-bar": "display:none; margin: 0;",
        "div.module-gallery-%s .auto-carousel-controls.auto-carousel-bar": "max-height: 0"
      }
    }
  },
