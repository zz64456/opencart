  },
  "sidePostsStyle": {
    "type": "Variable",
    "variable": "side_posts",
    "selector_prefix": ".module-blog_side_posts-%s"
  },
  "gridType": {
    "type": "Radio",
    "value": "auto",
    "php": true
  },
  "flexItems": {
    "type": "InputPair",
    "selector": ".module-blog_side_posts-%s .flex-grid",
    "properties": {
      "first": "--flex-grid-width: %spx",
      "second": "--flex-grid-max-col: calc((100% - (%s - 1) * var(--flex-grid-gap, 0px)) / %s)"
    },
    "value": {
      "first": "",
      "second": ""
    }
  },
  "flexItemGap": {
    "type": "Gap",
    "variableType": "gap",
    "selector": ".module-blog_side_posts-%s .flex-grid",
    "property": "--flex-grid-gap:%s"
  },
  "flexGrow": {
    "type": "Radio",
    "value": "1",
