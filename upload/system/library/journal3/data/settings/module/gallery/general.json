    "type": "Toggle",
    "value": "",
    "rules": {
      "true": {
        ".module-gallery-%s .auto-grid > .auto-carousel-buttons": "visibility: visible;"
      },
      "false": {
        ".module-gallery-%s .auto-grid > .auto-carousel-buttons": "visibility: hidden;"
      }
    }
  },
  "FaderStatus": {
    "type": "Radio",
    "value": "off",
    "rules": {
      "off": {
        ".module-gallery-%s .auto-grid > .auto-grid-items::before": "--fader-status: 0; display: none",
        ".module-gallery-%s .auto-grid > .auto-grid-items::after": "--fader-status: 0; display: none"
      },
      "both": {
        ".module-gallery-%s .auto-grid > .auto-grid-items::before": "--fader-status: 1; display: block",
        ".module-gallery-%s .auto-grid > .auto-grid-items::after": "--fader-status: 1; display: block"
      },
      "start": {
        ".module-gallery-%s .auto-grid > .auto-grid-items::before": "--fader-status: 1; display: block",
        ".module-gallery-%s .auto-grid > .auto-grid-items::after": "--fader-status: 0; display: none"
      },
      "end": {
        ".module-gallery-%s .auto-grid > .auto-grid-items::before": "--fader-status: 0; display: none",
        ".module-gallery-%s .auto-grid > .auto-grid-items::after": "--fader-status: 1; display: block"
      }
    }
  },
  "FaderColor": {
    "type": "Color",
    "selector": ".module-gallery-%s .auto-grid .auto-grid-items::before, .module-gallery-%s .auto-grid .auto-grid-items::after",
    "property": "--fader-color: %s"
  },
  "FaderWidth": {
    "type": "InputPair",
    "selector": ".module-gallery-%s .auto-grid .auto-grid-items::before, .module-gallery-%s .auto-grid .auto-grid-items::after",
    "properties": {
      "first": "--fader-width-left:%spx",
      "second": "--fader-width-right:%spx",
      "value": {
        "first": "",
        "second": ""
      }
    }
  },
  "autoCarouselStyle": {
    "type": "Variable",
    "variable": "auto_carousel",
    "selector_prefix": ".module-gallery-%s "
  },
  "AutoCarouselScrollerType": {
    "type": "Radio",
    "value": "",
    "rules": {
      "custom": {
        "div.module-gallery-%s .auto-grid": "--scrollbar-top-space: var(--scrollbar-top-spacing, 0px); --scrollbar-bottom-space: var(--scrollbar-bottom-spacing, 0px)",
