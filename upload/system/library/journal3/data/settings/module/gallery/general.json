      },
      "none": {
        "div.module-gallery-%s .module-item > a::after": "--display:none;",
        "div.module-gallery-%s .module-item > a::before": "display:none"
      }
    }
  },
  "iconAlways": {
    "type": "Toggle",
    "value": "",
    "rules": {
      "true": {
