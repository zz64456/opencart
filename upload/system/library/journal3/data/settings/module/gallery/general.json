      "tl": {
        "div.module-gallery-%s .module-item > a::after": "align-items:flex-start; justify-content: flex-start;"
      },
      "tc": {
        "div.module-gallery-%s .module-item > a::after": "align-items:flex-start; justify-content: center;"
      },
      "tr": {
        "div.module-gallery-%s .module-item > a::after": "align-items:flex-start; justify-content: flex-end;"
      },
      "c": {
        "div.module-gallery-%s .module-item > a::after": "align-items:center; justify-content: center;"
      },
      "bl": {
        "div.module-gallery-%s .module-item > a::after": "align-items:flex-end; justify-content: flex-start;"
      },
