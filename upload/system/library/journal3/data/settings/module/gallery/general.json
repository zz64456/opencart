  "ImageScale": {
    "type": "Toggle",
    "value": "",
    "rules": {
      "true": {
        "div.module-gallery-%s .module-item > a:hover img": "transform:scale(1.12)"
      },
      "false": {
        "div.module-gallery-%s .module-item > a img": "transform:scale(1)"
      }
    }
  },
  "ImageShapeClipPath": {
    "type": "Text",
    "value": "",
    "rules": {
      "@": {
        "div.module-gallery-%s .module-item > a": "%s"
      }
    }
  },
  "ImageTitleThumbs": {
    "type": "Toggle",
    "value": "",
    "rules": {
      "true": {
        "div.module-gallery-%s .gallery-image-caption": "display:block"
      },
      "false": {
        "div.module-gallery-%s .gallery-image-caption": "display:none"
      }
    }
  },
  "ImageTitleThumbsFont": {
    "type": "Font",
    "selector": "div.module-gallery-%s .gallery-image-caption"
  },
