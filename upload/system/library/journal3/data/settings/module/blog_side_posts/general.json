      },
      "content": {
        "div.module-blog_side_posts-%s": "--auto-carousel-controls-max-width: min(min(var(--content-max-width, 100%) - var(--scroll-padding, 0px) * 2, var(--content-max-width, 100%) - var(--content-gutter, 0) * 2), 100% - var(--scroll-padding, 0px) * 2)"
      }
    }
  },
  "AutoScrollbarAlign": {
