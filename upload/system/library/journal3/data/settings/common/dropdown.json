{
  "Align": {
    "type": "Radio",
    "value": "",
    "rules": {
      "left": {
        "&.drop-menu > .j-dropdown": "left: calc(var(--menu-item-gap, 0px) / 2); right: auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-start; --align-scroll: var(--align-scroll-start, 0); --translate-x: 0;",
        "&.drop-menu:first-child > .j-dropdown": "left: 0px; right: auto;",
        "&.drop-menu:last-child > .j-dropdown": "left: calc(var(--menu-item-gap, 0px) / 2); right: auto;",
        "&.dropdown > .j-dropdown::before": "--triangle-right: var(--triangle-right-offset, 0px); --triangle-left: var(--triangle-left-offset, 0px)",
        "&.mega-custom > .j-dropdown": "left: calc(var(--menu-item-gap, 0px) / 2); right: auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-start; --align-scroll: var(--align-scroll-start, 0); --translate-x: 0;",
        "&.mega-custom:first-child > .j-dropdown": "left: 0px; right: auto;",
        "&.mega-custom:last-child > .j-dropdown": "left: calc(var(--menu-item-gap, 0px) / 2); right: auto;",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      },
      "center": {
        "&.drop-menu > .j-dropdown": "left:50%; right:auto; transform: translate3d(-50%, var(--dropdown-from), 0); align-items: center; --align-scroll: var(--align-scroll-center, 0); --translate-x: -50%;",
        "&.drop-menu:first-child > .j-dropdown": "left:50%; right:auto;",
        "&.drop-menu:last-child > .j-dropdown": "left:50%; right:auto;",
        "&.dropdown > .j-dropdown::before": "--triangle-right: 0px; --triangle-left: 0px",
        "&.mega-custom > .j-dropdown": "left:50%; right:auto; transform: translate3d(-50%, var(--dropdown-from), 0); align-items: center; --align-scroll: var(--align-scroll-center, 0); --translate-x: -50%;",
        "&.mega-custom:first-child > .j-dropdown": "left:50%; right:auto;",
        "&.mega-custom:last-child > .j-dropdown": "left:50%; right:auto;",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0;",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      },
      "right": {
        "&.drop-menu > .j-dropdown": "right:calc(var(--menu-item-gap, 0px) / 2); left:auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-end; --align-scroll: var(--align-scroll-end, 0); --translate-x: -100%;",
        "&.drop-menu:first-child > .j-dropdown": "right:calc(var(--menu-item-gap, 0px) / 2); left:auto;",
        "&.drop-menu:last-child > .j-dropdown": "right:0px; left:auto;",
        "&.dropdown > .j-dropdown::before": "--triangle-right: var(--triangle-right-offset, 0px); --triangle-left: var(--triangle-left-offset, 0px)",
        "&.mega-custom > .j-dropdown": "right:calc(var(--menu-item-gap, 0px) / 2); left:auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-end; --align-scroll: var(--align-scroll-end, 0); --translate-x: -100%;",
        "&.mega-custom:first-child > .j-dropdown": "right:calc(var(--menu-item-gap, 0px) / 2); left:auto;",
        "&.mega-custom:last-child > .j-dropdown": "right:0px; left:auto;",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      }
    },
    "rtlRules": {
      "left": {
        "&.drop-menu > .j-dropdown": "left: auto; right: calc(var(--menu-item-gap, 0px) / 2); transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-start; --align-scroll: var(--align-scroll-start, 0); --translate-x: 0;",
        "&.drop-menu:first-child > .j-dropdown": "left: auto; right: 0px;",
        "&.drop-menu:last-child > .j-dropdown": "left: auto; right: calc(var(--menu-item-gap, 0px) / 2);",
        "&.dropdown > .j-dropdown::before": "--triangle-right: var(--triangle-right-offset, 0px); --triangle-left: var(--triangle-left-offset, 0px)",
        "&.mega-custom > .j-dropdown": "left: auto; right: calc(var(--menu-item-gap, 0px) / 2); transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-start; --align-scroll: var(--align-scroll-start, 0); --translate-x: 0;",
        "&.mega-custom:first-child > .j-dropdown": "left: auto; right: 0px;",
        "&.mega-custom:last-child > .j-dropdown": "left: auto; right: calc(var(--menu-item-gap, 0px) / 2);",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      },
      "center": {
        "&.drop-menu > .j-dropdown": "left:50%; right:auto; transform: translate3d(-50%, var(--dropdown-from), 0); align-items: center; --align-scroll: var(--align-scroll-center, 0); --translate-x: 50%;",
        "&.drop-menu:first-child > .j-dropdown": "left:50%; right:auto;",
        "&.drop-menu:last-child > .j-dropdown": "left:50%; right:auto;",
        "&.dropdown > .j-dropdown::before": "--triangle-right: 0px; --triangle-left: 0px",
        "&.mega-custom > .j-dropdown": "left:50%; right:auto; transform: translate3d(-50%, var(--dropdown-from), 0); align-items: center; --align-scroll: var(--align-scroll-center, 0); --translate-x: 50%;",
        "&.mega-custom:first-child > .j-dropdown": "left:50%; right:auto;",
        "&.mega-custom:last-child > .j-dropdown": "left:50%; right:auto;",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0;",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      },
      "right": {
        "&.drop-menu > .j-dropdown": "left:calc(var(--menu-item-gap, 0px) / 2); right:auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-end; --align-scroll: var(--align-scroll-end, 0); --translate-x: 100%;",
        "&.drop-menu:first-child > .j-dropdown": "left:calc(var(--menu-item-gap, 0px) / 2); right:auto;",
        "&.drop-menu:last-child > .j-dropdown": "left:0px; right:auto;",
        "&.dropdown > .j-dropdown::before": "--triangle-right: var(--triangle-right-offset, 0px); --triangle-left: var(--triangle-left-offset, 0px)",
        "&.mega-custom > .j-dropdown": "left:calc(var(--menu-item-gap, 0px) / 2); right:auto; transform: translate3d(0, var(--dropdown-from), 0); align-items: flex-end; --align-scroll: var(--align-scroll-end, 0); --translate-x: 100%;",
        "&.mega-custom:first-child > .j-dropdown": "left:calc(var(--menu-item-gap, 0px) / 2)",
        "&.mega-custom:last-child > .j-dropdown": "left:0px; right:auto;",
        "&.drop-menu.animating > .j-dropdown": "--dropdown-from: 0",
        "&.mega-menu.animating > .j-dropdown": "--dropdown-from: 0"
      }
    }
  },

  "MenuStyle": {
    "type": "Variable",
    "variable": "menu",
    "selector_prefix": "&.dropdown"
  },
  "Font": {
    "type": "Font",
    "selector": "&.dropdown .j-menu > li > a, &.dropdown .j-menu > li > a > .links-text"
  },
  "FontHover": {
    "type": "Font",
    "selector": "&.dropdown .j-menu > li:hover > a, &.dropdown .j-menu > li:hover > a > .links-text"
  },
  "FontActive": {
    "type": "Font",
    "selector": "&.dropdown .j-menu > li.open > a, &.dropdown .j-menu > li.open > a > .links-text",
    "important": true
  },
  "Background": {
    "type": "Background",
    "selector": "&.dropdown .j-menu > li > a, &.dropdown .j-menu > li > a"
  },
  "BackgroundHover": {
    "type": "Background",
    "selector": "&.dropdown .j-menu > li:hover > a, &.dropdown .j-menu > li:hover > a"
  },
  "BackgroundActive": {
    "type": "Background",
    "selector": "&.dropdown .j-menu > li.open > a, &.dropdown .j-menu > li.open > a",
    "important": true
  },
  "IconColor": {
    "type": "Color",
    "selector": "&.dropdown .j-menu > li > a::before",
    "property": "color"
  },
  "IconHover": {
    "type": "Color",
    "selector": "&.dropdown .j-menu > li:hover > a::before",
    "property": "color"
  },
  "IconActive": {
    "type": "Color",
    "selector": "&.dropdown .j-menu > li.open > a::before",
    "property": "color"
  },
  "Divider": {
    "type": "Divider",
    "selector": "&.dropdown .j-menu > li"
  },
  "DropPadding": {
    "type": "Padding",
    "selector": "&.dropdown:not(.mega-menu):not(#search) .j-dropdown > .j-menu, &.dropdown.flyout .flyout-menu > .j-menu"
  },

  "DropPaddingNew": {
    "type": "Gap",
    "variableType": "gap",
    "value": "",
    "property": "--dropdown-padding:%s",
    "selector": ".j-menu"
  },

  "DropBackground": {
    "type": "Background",
    "selector": "&.dropdown:not(.mega-menu):not(#search) .j-dropdown .j-menu",
    "value": {
      "background-color": "rgba(255,255,255,1)"
    }
  },

  "Width": {
    "type": "InputNumber",
    "value": "",
    "rules": {
      "@": {
        "&.dropdown:not(.mega-menu):not(#search) > .j-dropdown": "--dropdown-width: %spx; width: %spx"
      }
    }
  },
  "WidthMainMenu": {
    "type": "InputNumber",
    "value": "",
    "rules": {
      "@": {
        "&.dropdown:not(.mega-menu):not(#search).main-menu-item .j-dropdown": "--dropdown-width-main-menu: %spx; width: %spx"
      }
    }
  },
  "Height": {
    "type": "InputNumber",
    "rules": {
      "@": {
        "&.dropdown:not(.mega-menu):not(#search) .j-menu > li": "min-height:%spx",
        "&.dropdown:not(.mega-menu):not(#search) .j-menu > li > a": "min-height:%spx"
      }
    }
  },
  "SublevelOffset": {
    "type": "InputNumber",
    "value": "",
    "selector": "&.main-menu-item .j-dropdown",
    "property": "--sub-level-offset: %spx"
  },
  "Shadow": {
    "type": "Shadow",
    "selector": "&.dropdown:not(.mega-menu):not(#search):not(.panel-active) .j-menu"
  },
  "BorderRadius": {
    "type": "BorderRadius",
    "selector": "&.dropdown:not(.mega-menu):not(#search):not(.panel-active) .j-menu"
  },
  "BorderRadius2": {
    "type": "BorderRadius",
    "selector": "&.dropdown:not(.mega-menu):not(#search):not(.panel-active) .j-menu .j-menu"
  },
  "MenuTriangle": {
    "include": "triangle",
    "selector_prefix": "&.dropdown:not(.mega-fullwidth)"
  }
}
