    "selector": "html"
  },
  "GlobalFontFamily": {
    "type": "Font",
    "selector": "body"
  },
  "Font": {
    "type": "Font",
    "selector": "body"
  },
  "GlobalFont": {
    "type": "Font",
    "selector": ""
  },
  "Headings": {
    "type": "Font",
    "selector":"h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6"
  },
  "FontRendering": {
    "type": "Select",
    "value": "",
    "property": "-webkit-font-smoothing"
  },
  "Link": {
