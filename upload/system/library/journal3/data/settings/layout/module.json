{
  "background": {
    "type": "Background",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
  "margin": {
    "type": "Margin",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s:not(:empty)",
    "important": "true"
  },
  "padding": {
    "type": "Padding",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s:not(:empty)",
    "important": "true"
  },
  "border": {
    "type": "Border",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
  "borderRadius": {
    "type": "BorderRadius",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
  "shadow": {
    "type": "Shadow",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
  "outline": {
    "type": "Outline",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
  "containerStyle": {
    "type": "Variable",
    "variable": "container",
    "selector_prefix": ".site-wrapper .grid-rows > .grid-row > .grid-cols-wrapper > .grid-cols > .grid-col > .grid-items > .grid-item.grid-module-%s-%s-%s-%s",
    "important": "true"
  },
    "gridItemMinHeight": {
    "type": "Input",
    "value": "",
    "property": "min-height: %spx",
    "selector": "div.grid-items > .grid-item.grid-module-%s-%s-%s-%s"
  },
  "gridItemMaxWidth": {
    "type": "Input",
    "value": "",
    "property": "max-width: %spx",
    "selector": ".grid-items > .grid-item.grid-module-%s-%s-%s-%s"
  },
  "gridItemOffset": {
    "type": "Input",
    "value": "",
    "rules": {
      "@": {
        ".grid-items > .grid-item.grid-module-%s-%s-%s-%s": "transform: translate3d(0,%s%,0);"
      }
    }
  },
  "gridItemAlignSelf": {
    "type": "Radio",
    "value": "",
    "property": "align-self: %s",
    "selector": ".grid-items > .grid-item.grid-module-%s-%s-%s-%s"
  },
  "visibility": {
    "type": "Toggle",
    "value": "true",
    "rules": {
      "true": {
        ".grid-items > .grid-item.grid-module-%s-%s-%s-%s": "display: flex"
      },
      "false": {
        ".grid-items > .grid-item.grid-module-%s-%s-%s-%s": "display:none"
      }
    }
  },
  "order": {
    "type": "Input",
    "value": "",
    "property": "order: %s",
    "selector": ".grid-items > .grid-item.grid-module-%s-%s-%s-%s"
  },
  "gridItemZIndex": {
    "type": "InputNumber",
    "value": "",
    "property": "z-index: %s",
    "selector": ".grid-items > .grid-item.grid-module-%s-%s-%s-%s"
  },
  "customClass": {
    "type": "Input"
  },
  "customCss": {
    "type": "Input"
  }
}
