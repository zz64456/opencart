        ".module-gallery-%s .auto-grid .auto-grid-items.is-gliding a": "pointer-events: none"
      },
      "false": {
        ".module-gallery-%s .auto-grid": "margin-inline-start: unset; margin-inline-end: unset; --fr: 1fr",
        ".module-gallery-%s .auto-grid .auto-grid-items": "grid-auto-flow: row dense; overflow-x:initial; padding-inline-start:unset; padding-inline-end: unset",
        ".module-gallery-%s .auto-grid .auto-carousel-controls": "display:none;",
        ".module-gallery-%s .auto-grid .auto-carousel-items::after": "display:none;",
        ".module-gallery-%s .auto-grid .auto-grid-items.is-gliding a": "pointer-events: auto"
      }
    }
  },
  "autoCarouselRows": {
    "type": "InputNumber",
    "rules": {
      "@": {
        ".module-gallery-%s .auto-grid .auto-grid-items": " --grid-rows: %s"
      }
    }
  },
