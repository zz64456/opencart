  },
  "thumbOutlineHover": {
    "type": "Outline",
    "selector": "div.module-gallery-%s .module-item > a:hover img"
  },
  "thumbBorderRadius": {
    "type": "BorderRadius",
    "selector": "div.module-gallery-%s .module-item > a"
  },
  "FirstBorderRadius": {
