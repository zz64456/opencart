  "SearchIconWidth": {
    "type": "InputNumber",
    "value": "",
    "rules":{
      "@":{
        ".header-search .search-button": "min-width:min(%spx, 100%)"
      }
    }
  },
  "SearchPlaceholder": {
    "type": "InputLang",
    "value": "Search...",
    "php": true
  },
  "SearchCategories": {
    "type": "InputLang",
    "value": "All",
    "php": true
  },
  "SearchCategoriesMinWidth": {
    "type": "InputNumber",
    "value":"",
    "rules":{
      "@":{
        ".header-search .search-categories-button": "min-width:%spx"
      }
    }
  },
  "SearchPlaceholderColor": {
    "type": "Color",
    "rules": {
      "@": {
        "#search input::-webkit-input-placeholder": "color: %s",
        "#search input::-moz-input-placeholder": "color: %s",
        "#search input:-ms-input-placeholder": "color: %s"
      }
    }
  },

  "SearchCategoriesSelectorStatus": {
    "type": "Toggle",
    "value": "true",
    "rules": {
      "false": {
        ".header-search > input":"border-top-left-radius: inherit; border-bottom-left-radius: inherit;"
      }
    },
    "rtlRules": {
      "false": {
        ".header-search > input":"border-top-right-radius: inherit; border-bottom-right-radius: inherit;"
      }
    },
    "php": true
  },

  "SearchCategoriesWidth": {
    "type": "InputNumber",
    "value": "",
    "selector": ".search-categories-button",
    "property": "--max-width: %spx"
  },

  "SearchCategoriesBorderRadius": {
    "type": "BorderRadius",
    "selector": ".search-categories",
    "important": "true"
  },

  "SearchCategoriesSelectorVisibility": {
    "type": "Toggle",
    "value": "true",
    "rules": {
      "true": {
        ".search-categories": "display: flex",
        ".header-search > span": "border-radius: 0"
      },
      "false": {
        ".search-categories": "display: none",
        ".search-categories + span": "border-top-left-radius: inherit; border-bottom-left-radius: inherit;",
        ".search-categories + label + span": "border-top-left-radius: inherit; border-bottom-left-radius: inherit;",
        ".search-categories + label + input": "border-top-left-radius: inherit; border-bottom-left-radius: inherit;"
      }
    }
  },

  "SearchCategoriesType": {
    "type": "Radio",
    "value": "all",
    "php": true
  },
  "SearchCategoriesSelectorBG": {
    "type": "Background",
    "selector": ".search-categories"
  },
  "SearchCategoriesSelectorBGHover": {
    "type": "Background",
    "selector": ".header-search:hover .search-categories"
  },
  "SearchCategoriesSelectorFont": {
