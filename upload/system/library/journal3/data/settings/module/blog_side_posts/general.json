  },
  "carouselPhone": {
    "type": "Toggle",
    "value": "",
    "php": true
  },
  "carouselStyle": {
    "type": "Variable",
    "variable": "carousel",
    "selector_prefix": ".module-blog_side_posts-%s "
  },
  "accordionStyle": {
    "type": "Variable",
    "variable": "accordion",
    "selector_prefix": ".module-blog_side_posts-%s "
  },
  "tabsStyle": {
    "type": "Variable",
    "variable": "tabs",
    "selector_prefix": ".module-blog_side_posts-%s "
