  },
  "itemFontActive": {
    "type": "Font",
    "selector": ".module-filter-%s label input:checked + .links-text, .module-filter-%s label input:checked + img + .links-text"
  },
  "imageBorder": {
    "type": "Border",
    "selector": ".module-filter-%s label img"
  },
  "imageBorderHover": {
    "type": "Color",
